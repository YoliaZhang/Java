package test;

interface IntCompare{
	public int cmp(int a, int b);
}
class Cmp1 implements IntCompare{
	public int cmp(int a,int b) {
		if(a>b) {
			return 1;
		}
		else if (a<b) {
			return -1;
		}
		else {
			return 0;
		}
	}
}
class Cmp2 implements IntCompare{
	public int cmp(int a,int b) {
		if(a<b) {
			return 1;
		}
		else if (a>b) {
			return -1;
		}
		else {
			return 0;
		}
	}
}
public class Test{
	public static void insertSort(int []a, IntCompare cmp){
		if(a!=null) {
			for(int i=1;i<a.length;i++) {
				int temp=a[i],j=i;
				if(cmp.cmp(a[j-1],temp)==1) {
					while(j>=1&&cmp.cmp(a[j-1], temp)==1) {
						a[j]=a[j-1];
						j--;
					}
				}
				a[j]=temp;
			}
		}
	}

public static void main(String[] args) {
	int[]array1= {3,19,40,4,7,1};
	insertSort(array1,new Cmp1());
	System.out.println("ÉýÐò");
	for(int i=0;i<array1.length;i++) {
		System.out.print(array1[i]+" ");
	}
	System.out.println();
	int[]array2= {3,19,40,4,7,1};
	insertSort(array1,new Cmp2());
	System.out.println("½µÐò");
	for(int i=0;i<array2.length;i++) {
		System.out.print(array2[i]+" ");
	}
}
}